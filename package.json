{
  "name": "vue3-emoji",
  "private": false,
  "author": "ADKCodeXD",
  "description": "一个简单的Emoji表情选择组件",
  "version": "2.0.0",
  "license": "MIT",
  "main": "dist/Vue3Emoji.umd.js",
  "module": "dist/Vue3Emoji.es.js",
  "style": "dist/style.css",
  "type": "module",
  "bugs": {
    "url": "https://github.com/ADKcodeXD/Vue3-Emoji/issues"
  },
  "homepage": "https://github.com/ADKcodeXD/Vue3-Emoji",
  "keywords": [
    "vue3-emoji",
    "emoji",
    "vue3-emoji-component",
    "emoji-component",
    "vue3emoji"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "dist/Vue3Emoji.es.js",
      "require": "dist/Vue3Emoji.umd.js"
    },
    "./*": "./*"
  },
  "types": "dist/src/components/V3Emoji.vue.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint src/**/* --ext '.js,.ts,.vue' --no-error-on-unmatched-pattern",
    "lintfix": "eslint --fix --ext '.js,.ts,.vue' src/**/* --no-error-on-unmatched-pattern"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "@vitejs/plugin-vue": "^2.3.3",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.1.1",
    "prettier": "^2.7.1",
    "prettier-eslint": "^15.0.1",
    "sass": "^1.53.0",
    "scss": "^0.2.4",
    "scss-loader": "^0.0.1",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.9.2",
    "vite": "^2.9.9",
    "vite-plugin-dts": "^2.3.0",
    "vue": "^3.0.0",
    "vue-tsc": "^0.38.3"
  }
}
